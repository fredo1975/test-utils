pipeline {
    agent any
    tools {
        maven 'Maven 3.3.9'
        jdk 'jdk8'
    }
    environment {
    	VERSION = readMavenPom().getVersion()
    	def pom = readMavenPom file: 'pom.xml'
    	def NVERSION = pom.version.replace("-SNAPSHOT", "")
    }
    stages {
        stage ('Build') {
		 		steps {
		 			withMaven(mavenSettingsConfig: 'MySettings') {
            			sh '''
            				mvn jgitflow:release-finish
            			'''
		    		}
		    	}
        }
    }
}
